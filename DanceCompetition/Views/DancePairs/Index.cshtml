@model IEnumerable<DanceCompetition.Models.DancePair>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.grade1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.grade2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.grade3)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.grade1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.grade2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.grade3)
                    </td>
                </tr>
        }
    </tbody>
</table>
