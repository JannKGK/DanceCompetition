@model DanceCompetition.Models.DancePairViewModel

@{
    ViewData["Title"] = "Contestants";
}

<h1>Results</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.StillCompeting)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.name</td>
            </tr>
        }
    </tbody>
</table>